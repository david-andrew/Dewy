// fizbuzz that works with the current version of dewy
loop i in [0..100)
{
    printed_words = false
    loop tap in [3,5..7)
    {
        if i % tap =? 0 
        { 
            print(if tap =? 3 'Fizz' else 'Buzz')
            printed_words = true
        }
    }
    if printed_words {} else print(i)
    printl()
}