// fizbuzz that works with the current version of dewy
//multiples = [3 5]       //TODO on variable typing for loop
//words = ['Fizz' 'Buzz'] //TODO on variable typing for loop
loop i in [0..100)
{
    print_num = true
    loop multiple in [3 5] and word in ['Fizz' 'Buzz']
    {
        if i % multiple =? 0 
        { 
            print(word)
            print_num = false
        }
    }
    if print_num print(i)
    printl()
}