// fizbuzz that works with the current version of dewy
multiples = [3 5]
words = ['Fizz' 'Buzz']
loop i in [0..100)
{
    print_num = true
    loop multiple in multiples and word in words
    {
        if i % multiple =? 0 
        { 
            print(word)
            print_num = false
        }
    }
    if print_num print(i)
    printl()
}