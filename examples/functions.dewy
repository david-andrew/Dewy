f0 = () => 0
printl('f0 = {f0}')

f1 = x => x + 1
printl('f1(5) = {f1(5)}')

f1b = (x) => x + 1
printl('f1b(5) = {f1b(5)}')

f1c = (x=2) => x + 1
printl('f1c = {f1c}')
printl('f1c(x=5) = {f1c(x=5)}')

f2 = (x, y) => x + y
printl('f2(5, 6) = {f2(5, 6)}')

f2b = (x, y=2) => x + y
printl('f2b(5) = {f2b(5)}')
printl('f2b(5, y=6) = {f2b(5, y=6)}')

f3 = (x, y, z) => x + y + z
printl('f3(5, 6, 7) = {f3(5, 6, 7)}')
